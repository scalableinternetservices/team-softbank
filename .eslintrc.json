{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "react/sort-comp": 0,
    "class-methods-use-this": 1,
    "react/require-default-props": 0,
    "no-else-return": 0,
    "no-param-reassign": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "react/jsx-one-expression-per-line": 0,
    "prettier/prettier": ["error"]
  }
}
